{
  "name": "ngx-libraries",
  "version": "10.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc-list-view": "./node_modules/.bin/compodoc -p projects/ngx-list-view/tsconfig.lib.json --output ./docs/ngx-list-view --name ngx-list-view",
    "build-list-view": "ng build --prod ngx-list-view",
    "pack-list-view": "cd dist/ngx-list-view && npm pack",
    "package-list-view": "npm run build-list-view && npm run pack-list-view",
    "doc-rest-auth": "./node_modules/.bin/compodoc -p projects/ngx-rest-auth/tsconfig.lib.json --output ./docs/ngx-rest-auth --name ngx-rest-auth",
    "build-rest-auth": "ng build --prod ngx-rest-auth",
    "pack-rest-auth": "cd dist/ngx-rest-auth && npm pack",
    "package-rest-auth": "npm run build-rest-auth && npm run pack-rest-auth",
    "doc-simple-accordion": "./node_modules/.bin/compodoc -p projects/ngx-simple-accordion/tsconfig.lib.json --output ./docs/ngx-simple-accordion  --name ngx-simple-accordion",
    "build-simple-accordion": "ng build --prod ngx-simple-accordion",
    "pack-simple-accordion": "cd dist/ngx-simple-accordion && npm pack",
    "package-simple-accordion": "npm run build-simple-accordion && npm run pack-simple-accordion",
    "doc-toastn": "./node_modules/.bin/compodoc -p projects/ngx-toastn/tsconfig.lib.json --output ./docs/ngx-toastn  --name ngx-toast",
    "build-toastn": "ng build --prod ngx-toastn",
    "pack-toastn": "cd dist/ngx-toastn && npm pack",
    "copy-toastn-css": "mkdir -p dist/ngx-toastn/css > /dev/null 2>&1 && cp projects/ngx-toastn/css/* dist/ngx-toastn/css",
    "copy-toastn-img": "mkdir -p dist/ngx-toastn/img > /dev/null 2>&1 && cp projects/ngx-toastn/img/* dist/ngx-toastn/img",
    "package-toastn": "npm run build-toastn && npm run copy-toastn-css && npm run copy-toastn-img && npm run pack-toastn",
    "doc-select-view": "./node_modules/.bin/compodoc -p projects/ngx-select-view/tsconfig.lib.json --output ./docs/ngx-select-view --name ngx-select-view",
    "build-select-view": "ng build --prod ngx-select-view",
    "pack-select-view": "cd dist/ngx-select-view && npm pack",
    "package-select-view": "npm run build-select-view && npm run pack-select-view"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~10.2.4",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~3.5.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.9.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  }
}