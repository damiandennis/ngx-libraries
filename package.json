{
  "name": "ngx-libraries",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc-list-view": "./node_modules/.bin/compodoc -p projects/ngx-list-view/tsconfig.lib.json --output ./docs/ngx-list-view --name ngx-list-view",
    "build-list-view": "ng build --prod ngx-list-view",
    "pack-list-view": "cd dist/ngx-list-view && npm pack",
    "package-list-view": "npm run build-list-view && npm run pack-list-view",
    "doc-yii2-rest": "./node_modules/.bin/compodoc -p projects/ngx-yii2-rest/tsconfig.lib.json --output ./docs/ngx-yii2-rest --name ngx-yii2-rest",
    "build-yii2-rest": "ng build --prod ngx-yii2-rest",
    "pack-yii2-rest": "cd dist/ngx-yii2-rest && npm pack",
    "package-yii2-rest": "npm run build-yii2-rest && npm run pack-yii2-rest",
    "doc-simple-accordion": "./node_modules/.bin/compodoc -p projects/ngx-simple-accordion/tsconfig.lib.json --output ./docs/ngx-simple-accordion  --name ngx-simple-accordion",
    "build-simple-accordion": "ng build --prod ngx-simple-accordion",
    "pack-simple-accordion": "cd dist/ngx-simple-accordion && npm pack",
    "package-simple-accordion": "npm run build-simple-accordion && npm run pack-simple-accordion",
    "doc-toastn": "./node_modules/.bin/compodoc -p projects/ngx-toastn/tsconfig.lib.json --output ./docs/ngx-toastn  --name ngx-toast",
    "build-toastn": "ng build --prod ngx-toastn",
    "pack-toastn": "cd dist/ngx-toastn && npm pack",
    "copy-toastn-css": "mkdir -p dist/ngx-toastn/css > /dev/null 2>&1 && cp projects/ngx-toastn/css/* dist/ngx-toastn/css",
    "copy-toastn-img": "mkdir -p dist/ngx-toastn/img > /dev/null 2>&1 && cp projects/ngx-toastn/img/* dist/ngx-toastn/img",
    "package-toastn": "npm run build-toastn && npm run copy-toastn-css && npm run copy-toastn-img && npm run pack-toastn"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/common": "^8.2.4",
    "@angular/compiler": "^8.2.4",
    "@angular/core": "^8.2.4",
    "@angular/forms": "^8.2.4",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@angular/router": "^8.2.4",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular-devkit/build-ng-packagr": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "^8.2.4",
    "@angular/language-service": "^8.2.4",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
}