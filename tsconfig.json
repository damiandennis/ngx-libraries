{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ngx-list-view": [
        "dist/ngx-list-view"
      ],
      "ngx-yii2-rest": [
        "dist/ngx-yii2-rest"
      ],
      "ngx-simple-accordion": [
        "dist/ngx-simple-accordion",
        "dist/ngx-simple-accordion"
      ],
      "ngx-toastn": [
        "dist/ngx-toastn"
      ],
      "ngx-select-view": [
        "dist/ngx-select-view"
      ]
    }
  }
}
